#include<stdio.h>
main(){

  float p1, p2, p3, media;

  printf("Vamos calcular a sua media, insira sua nota da prova 1");
    scanf("%f", &p1);

  printf("agora insira sua nota da prova 2");
    scanf("%f", &p2);

  printf("por ultimo a nota da prova 3");
    scanf("%f", &p3);

  media= (p1+p2+(p3*2))/3;

  printf(" a sua media de notas e igual a %f", media);

}
